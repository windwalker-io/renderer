{
    "name": "windwalker/renderer",
    "type": "windwalker-package",
    "description": "Windwalker Renderer package",
    "keywords": [
        "windwalker",
        "framework",
        "renderer",
        "blade",
        "edge",
        "mustach",
        "plates"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-renderer",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "windwalker/utilities": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "windwalker/test": "4.x-dev",
        "windwalker/dom": "4.x-dev",
        "windwalker/filesystem": "4.x-dev",
        "twig/twig": "^2.0",
        "illuminate/view": "^6.0||^7.0||^8.0||^9.0",
        "mustache/mustache": "^2.0",
        "league/plates": "^3.4"
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Windwalker\\Renderer\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Renderer\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-4": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
